{
  "name": "enki",
  "version": "0.0.1",
  "description": "manage translations based on formatmessage/ICU style data with an easy to use web frontend",
  "main": "index.js",
  "directories": {
    "example": "example"
  },
  "scripts": {
    "client:build": "NODE_ENV=production webpack --display-modules --display-chunks --progress --color --bail",
    "client:dev:hot": "webpack-dev-server --progress --color",
    "server:dev": "NODE_ENV=development node --harmony src",
    "server:dev:watch": "NODE_ENV=development nodemon --harmony src",
    "lint": "eslint --ext .jsx,.js src/",
    "lint:cli": "eslint --ext .jsx,.js",
    "lint:autofix": "eslint --fix --ext .jsx,.js src/",
    "lint:watch": "esw -w --ext .jsx,.js src",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cospired/enki.git"
  },
  "keywords": [
    "react-intl",
    "icu",
    "localisation",
    "internationalisation",
    "l10n",
    "i18n"
  ],
  "author": "Dirk Haage <dirk@cospired.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cospired/enki/issues"
