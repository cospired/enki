{
  "name": "enki",
  "version": "0.0.1",
  "description": "manage translations based on formatmessage/ICU style data with an easy to use web frontend",
  "main": "index.js",
  "directories": {
    "example": "example"
  },
  "scripts": {
    "dev": "NODE_ENV=development node --harmony src",
    "dev:watch": "NODE_ENV=development nodemon --harmony src",
    "lint": "eslint --ext .jsx,.js src/",
    "lint:cli": "eslint --ext .jsx,.js",
    "lint:autofix": "eslint --fix --ext .jsx,.js src/",
    "lint:watch": "esw -w --ext .jsx,.js src",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cospired/enki.git"
  },
  "keywords": [
    "react-intl",
    "icu",
    "localisation",
    "internationalisation",
    "l10n",
    "i18n"
  ],
  "author": "Dirk Haage <dirk@cospired.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cospired/enki/issues"
  },
  "homepage": "https://github.com/cospired/enki#readme",
  "dependencies": {
    "commander": "^2.9.0",
    "fs-extra": "^3.0.1",
    "glob": "^7.1.2",
    "lodash.merge": "^4.6.0",
    "storyboard": "^3.1.1",
    "storyboard-listener-console": "^3.1.1",
    "storyboard-listener-ws-server": "^3.1.1",
    "storyboard-preset-console": "^3.1.1"
  },
  "devDependencies": {
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-hapi": "^4.0.0",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "eslint-watch": "^3.1.0",
    "nodemon": "^1.11.0"
  }
}
